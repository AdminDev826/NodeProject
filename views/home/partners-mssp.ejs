<% include ../html/html_begin %>
<% include ../html/head_begin %>
  <meta name="description" content="MSSP Partners for <%= appdata.product %> Digital Risk Management">
  <link rel="stylesheet" href="/css/index.css">
  <title>MSSP Partners for <%= appdata.product %> Digital Risk Management</title>
<% include ../html/head_end %>
<% include ../html/body_begin %>

<% include ../home/components/navbar %>
<% include ../home/components/menubar %>

  <div class="site-body">


    <!-- Hero -->
    <section class="site-hero hero hero-light padding-top-80"
    style="background-image:url(/img/home/partners-mssp.jpg)">
      <div class="container">
        <div class="content">
          <h1>Managed Security Service Provider (MSSP) Partners</h1>
          <p class="section-desc white">Join our rapidly growing network of
            organizations that trust <%= appdata.product %> to deliver comprehensive security
            coverage to their clients.
          </p>
          <p>
              <a href="#" class="btn btn-lg btn-primary margin-bottom-20" data-target="#contactFormModal" data-toggle="modal">Apply</a>
          </p>
        </div>
      </div>
    </section>
    <!-- End Hero -->

    <!-- Feature -->
    <section class="section section-feature text-center bg-grey-300">
      <div class="container container-fluid">
        <div class="col-sm-12">
          <h3 class="section-title">Why <%= appdata.product %>?</h3>
        </div>
      </div>
    </section>
    <!-- Feature -->
    <section class="section section-feature section-small text-center section-primary">
      <div class="container container-fluid">
        <div class="row feature-group">
          <div class="col-sm-12 col-md-4 iconbox iconbox-left">
            <i class="icon iconbox-icon wb-mobile"></i>
            <div class="iconbox-content">
              <h3 class="iconbox-title">Fullstack Protection</h3>
              <p class="section-desc"><%= appdata.product %> offers protection across numerous
                digital and social channels to give your clients the widest, most effective
                protection for 'outside the perimeter' digital threats. From event security to protecting financial assets,
                <%= appdata.product %> is with you every step of the way.
              </p>
            </div>
          </div>
          <div class="col-sm-12 col-md-4 iconbox iconbox-left">
            <i class="icon iconbox-icon wb-cloud"></i>
            <div class="iconbox-content">
              <h3 class="iconbox-title">Seamless deployment at any scale</h3>
              <p class="section-desc"><%= appdata.product %> can operate anywhere in the
                world and monitor any number of locations and keywords so your clients
                can customize the exact program they need to be tangibly safer.
              </p>
            </div>
          </div>
          <div class="col-sm-12 col-md-4 iconbox iconbox-left">
            <i class="icon iconbox-icon wb-check"></i>
            <div class="iconbox-content">
              <h3 class="iconbox-title">Maximized ROI</h3>
              <p class="section-desc"><%= appdata.product %> does more than just protect
                our customers from vulnerabilities. Our monitoring platform
                guides users toward opportunities to help their brands excel on new levels.
              </p>
            </div>
          </div>
        </div>

      </div>
    </section>
    <!-- End Feature -->

     <!-- Feature -->
     <section class="section section-feature section-small text-center bg-grey-200">
       <div class="container container-fluid">
         <h3 class="section-title">Interested in becoming a partner?</h3>

         <p class="section-desc">Click below to find out if <%= appdata.product %> could be right for you</p>
         <a href="#" class="btn btn-lg btn-primary margin-bottom-20" data-target="#contactFormModal" data-toggle="modal">Apply</a>

       </div>
     </section>
     <!-- End Feature -->


  </div>

<% include ../home/components/footer_home %>
<% include ../home/components/contact_form %>

<script src="/js/global/global.js"></script>
<script src="/js/index.js"></script>

<% include ../html/body_end %>
<% include ../html/html_end %>
