<% include ../html/html_begin %>
<% include ../html/head_begin %>
  <meta name="description" content="Resellers">
  <link rel="stylesheet" href="/css/index.css">
  <title>Resellers</title>
<% include ../html/head_end %>
<% include ../html/body_begin %>

<% include ../home/components/navbar %>
<% include ../home/components/menubar %>

  <div class="site-body">


    <!-- Hero -->
    <section class="site-hero hero hero-light padding-top-80"
    style="background-image:url(/img/home/partners-reseller.jpg)">
      <div class="container">
        <div class="content">
          <h1>Resellers</h1>
          <p class="section-desc white">Tap into one of the fastest growing
            software industries with the best product on the market.
          </p>
          <p>
            <a href="#" class="btn btn-lg btn-primary margin-bottom-20" data-target="#contactFormModal" data-toggle="modal">Apply</a>
          </p>
        </div>
      </div>
    </section>
    <!-- End Hero -->

    <!-- Feature -->
    <section class="section section-feature text-center bg-grey-300">
      <div class="container container-fluid">
        <div class="col-sm-12">
          <h3 class="section-title">Why <%= appdata.product %>?</h3>
        </div>
      </div>
    </section>
    <!-- Feature -->
    <section class="section section-feature section-small text-center section-primary">
      <div class="container container-fluid">
        <div class="row feature-group">
          <div class="col-sm-12 col-md-4 iconbox iconbox-left">
            <i class="icon iconbox-icon wb-mobile"></i>
            <div class="iconbox-content">
              <h3 class="iconbox-title">Increase Revenues</h3>
              <p class="section-desc"><%= appdata.product %>’s platform is tailored to
                serve Fortune 500 clients and support all of their cybersecurity and
                business intelligence needs. Access high profile clients and drive
                greater revenues with <%= appdata.product %>’s enterprise-enabled dashboard.
              </p>
            </div>
          </div>
          <div class="col-sm-12 col-md-4 iconbox iconbox-left">
            <i class="icon iconbox-icon wb-cloud"></i>
            <div class="iconbox-content">
              <h3 class="iconbox-title">Unique feature offerings</h3>
              <p class="section-desc"><%= appdata.product %>’s toolkit contains the
                best search, monitor, and investigation software in the business
                and delivers results to every client.
              </p>
            </div>
          </div>
          <div class="col-sm-12 col-md-4 iconbox iconbox-left">
            <i class="icon iconbox-icon wb-check"></i>
            <div class="iconbox-content">
              <h3 class="iconbox-title">Drive long term growth</h3>
              <p class="section-desc"><%= appdata.product %>’s subscription and data
                management model drives recurring revenues while maximizing every
                client’s return on investment with customized plans and active support.
              </p>
            </div>
          </div>
        </div>
      </div>
    </section>
    <!-- End Feature -->

     <!-- Feature -->
     <section class="section section-feature section-small text-center">
       <div class="container container-fluid">
         <h3 class="section-title">Interested in becoming a reseller?</h3>

         <p class="section-desc">Click below to find out if <%= appdata.product %> could be right for you</p>
         <a href="#" class="btn btn-lg btn-primary margin-bottom-20" data-target="#contactFormModal" data-toggle="modal">Apply</a>

       </div>
     </section>
     <!-- End Feature -->




  </div>

<% include ../home/components/footer_home %>
<% include ../home/components/contact_form %>

<script src="/js/global/global.js"></script>
<script src="/js/index.js"></script>
<% include ../html/body_end %>
<% include ../html/html_end %>
